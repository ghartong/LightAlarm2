<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--
    /***************************************************************
    /*
    /* YOU MUST HAVE A DATE, MSG, AND IMG FOR EACH HOLIDAY */
    /* Element number must match.. date[2] => img[2] => msg[2]
    /*
    /***************************************************************
    These Holidays will show up on the detail view
    -->

    <!-- Holiday date used to determine if TODAY is a holiday [CHAR LIMIT=20]-->
    <string-array name="holiday_dates">
        <item>December 19</item>
        <item>December 20</item>
        <item>January 1</item>
    </string-array>

    <!-- Holiday message to display [CHAR LIMIT=20]-->
    <string-array name="holiday_msgs">
        <item>Happy 4th of July</item>
        <item>Merry Christmas</item>
        <item>Happy New Years</item>
    </string-array>

    <!-- Filename (minus extension) of the Drawable to display [CHAR LIMIT=20]-->
    <string-array name="holiday_imgs">
        <item>bikini_july</item>
        <item>bikini_santa</item>
        <item>bikini_baby_newyear</item>
    </string-array>

</resources>